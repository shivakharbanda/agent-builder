  {
    "model": "projects.project",
    "pk": 1,
    "fields": {
      "name": "Call Center Analytics",
      "description": "Intent classification and sentiment analysis for customer service calls",
      "created_by": 1,
      "is_active": true
    }
  },
  {
    "model": "agents.agent",
    "pk": 1,
    "fields": {
      "name": "Intent Classifier Agent",
      "description": "Classifies customer intent from call transcripts using predefined categories",
      "return_type": "structured",
      "schema_definition": {
        "type": "object",
        "properties": {
          "intent": {"type": "string", "enum": ["complaint", "inquiry", "compliment", "refund_request", "technical_support"]},
          "confidence": {"type": "number", "minimum": 0, "maximum": 1},
          "reasoning": {"type": "string"}
        },
        "required": ["intent", "confidence"]
      },
      "project": 1,
      "created_by": 1,
      "is_active": true
    }
  },
  {
    "model": "agents.prompt",
    "pk": 1,
    "fields": {
      "agent": 1,
      "prompt_type": "system",
      "content": "You are an expert at analyzing customer service calls. Your task is to classify the customer's intent based on the call transcript. Available categories: {categories}. Always respond with structured JSON containing the intent, confidence score (0-1), and reasoning for your classification.",
      "placeholders": {
        "categories": "List of available intent categories (complaint, inquiry, compliment, refund_request, technical_support)"
      },
      "created_by": 1,
      "is_active": true
    }
  },
  {
    "model": "agents.prompt",
    "pk": 2,
    "fields": {
